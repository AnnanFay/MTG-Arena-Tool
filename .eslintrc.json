{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "allowEmptyCatch": true,
    "no-cond-assign": 0,
    "no-console": "off",
    "complexity": ["warn", 20],
    "max-statements": ["warn", 100],
    "indent": ["error", 2]
  }
}
